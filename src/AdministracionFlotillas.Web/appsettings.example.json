{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "OracleConnection": "Data Source=(description=(retry_count=20)(retry_delay=3)(address=(protocol=tcps)(port=1522)(host=YOUR_HOST.oraclecloud.com))(connect_data=(service_name=YOUR_SERVICE_NAME.adb.oraclecloud.com))(security=(ssl_server_dn_match=yes)));User Id=YOUR_USER_ID;Password=YOUR_PASSWORD;Connection Timeout=60;"
  },
  "OracleSettings": {
    "TnsAdmin": ""
  },
  "DatabaseSettings": {
    "UseMockData": true,
    "Note": "Cuando tengas Oracle configurado, cambia UseMockData a false y actualiza ConnectionStrings"
  }
}
